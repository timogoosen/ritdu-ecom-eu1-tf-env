provider "aws" {
  version = "2.70.0"
  region  = local.main_vpc_region
  profile = "rimu"

  assume_role {
    role_arn = var.prefix == "rimu" ? "" : "arn:aws:iam::${local.main_account_id}:role/administrators"
  }
}

